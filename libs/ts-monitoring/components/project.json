{
    "root": "libs/ts-monitoring/components",
    "sourceRoot": "libs/ts-monitoring/components/src",
    "projectType": "library",
    "tags": [],
    "targets": {
        "build": {
            "executor": "@nrwl/web:rollup",
            "outputs": ["{options.outputPath}"],
            "options": {
                "outputPath": "dist/libs/ts-monitoring/components",
                "tsConfig": "libs/ts-monitoring/components/tsconfig.lib.json",
                "project": "libs/ts-monitoring/components/package.json",
                "entryFile": "libs/ts-monitoring/components/src/index.ts",
                "external": ["react/jsx-runtime"],
                "rollupConfig": "@nrwl/react/plugins/bundle-rollup",
                "compiler": "babel",
                "assets": [
                    {
                        "glob": "libs/ts-monitoring/components/README.md",
                        "input": ".",
                        "output": "."
                    }
                ]
            }
        },
        "lint": {
            "executor": "@nrwl/linter:eslint",
            "outputs": ["{options.outputFile}"],
            "options": {
                "lintFilePatterns": ["libs/ts-monitoring/components/**/*.{ts,tsx,js,jsx}"]
            }
        },
        "test": {
            "executor": "@nrwl/jest:jest",
            "outputs": ["coverage/libs/ts-monitoring/components"],
            "options": {
                "jestConfig": "libs/ts-monitoring/components/jest.config.js",
                "passWithNoTests": true
            }
        }
    }
}
